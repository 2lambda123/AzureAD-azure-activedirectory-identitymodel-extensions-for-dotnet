<Project>
  <PropertyGroup>
    <TestTargets Condition="'$(LocalBuild)' != 'True' and '$(TargetNet9)' != 'True'">net461;net462;net472;netcoreapp2.1;net6.0;net8.0</TestTargets>
    <TestTargets Condition="'$(LocalBuild)' != 'True' and '$(TargetNet9)' == 'True'">net461;net462;net472;netcoreapp2.1;net6.0;net8.0;net9.0</TestTargets>
    <TestTargets Condition="'$(LocalBuild)' == 'True' and '$(TargetNet9)' != 'True'">net461;netcoreapp2.1;net8.0</TestTargets>
    <TestTargets Condition="'$(LocalBuild)' == 'True' and '$(TargetNet9)' == 'True'">net461;netcoreapp2.1;net8.0;net9.0</TestTargets>
    <TestOnlyCoreTargets>netcoreapp2.1</TestOnlyCoreTargets>
  </PropertyGroup>
</Project>
